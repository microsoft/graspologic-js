{
	"name": "@graspologic/monorepo",
	"version": "0.6.0",
	"private": true,
	"description": "Efficient WebGL-based Graph Data visualization + React Componentry",
	"repository": "https://github.com/microsoft/Graspologic-js",
	"author": "Chris Trevino (chtrevin@microsoft.com)",
	"contributors": [
		"David Tittsworth (datittsw@microsoft.com)",
		"Nathan Evans (naevans@microsoft.com)"
	],
	"license": "MIT",
	"scripts": {
		"preinstall": "npx only-allow yarn",
		"clean:": "yarn workspaces foreach -piv run clean",
		"build:": "yarn workspaces foreach -pivt run build",
		"bundle:": "yarn workspaces foreach -pv run bundle",
		"start:": "yarn workspaces foreach -piv run start",
		"test:": "yarn workspaces foreach -piv run test",
		"release:": "yarn workspaces foreach run npm publish --tolerate-republish --access public",
		"lint:": "essex lint --docs",
		"lint:fix": "essex lint --docs --fix",
		"unit:test": "jest --ci --coverage",
		"prettify": "essex prettify",
		"start_glsl": "yarn start:glsl",
		"start_storybook": "yarn start:storybook",
		"build_glsl": "yarn build:glsl",
		"git_is_clean": "essex git-is-clean",
		"ci": "npm-run-all -s build_glsl -p build: bundle: lint: test: unit:test -s git_is_clean"
	},
	"engines": {
		"node": ">= 12"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.5",
		"@babel/preset-typescript": "^7.12.1",
		"@essex/eslint-config": "^10.0.2",
		"@essex/eslint-plugin": "^10.0.2",
		"@essex/jest-config": "^11.0.0",
		"@essex/prettier-config": "^10.0.0",
		"@essex/scripts": "^12.0.0",
		"@types/enzyme": "^3.10.7",
		"@types/eslint": "^7.2.4",
		"@types/invariant": "^2.2.34",
		"@types/jest": "^26.0.15",
		"@types/node": "^14.14.2",
		"@types/prettier": "^2.1.5",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"applicationinsights": "^1.8.7",
		"colors": "^1.4.0",
		"dts-bundle": "^0.7.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^7.11.0",
		"eslint-import-resolver-node": "^0.3.4",
		"eslint-plugin-header": "^3.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^4.3.0",
		"jest": "^26.6.0",
		"jest-webgl-canvas-mock": "^0.2.3",
		"lint-staged": "^10.4.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"regenerator-runtime": "^0.13.7",
		"rollup": "^2.32.1",
		"shx": "^0.3.2",
		"typedoc-plugin-markdown": "^2.4.2",
		"typescript": "4.5.2"
	},
	"resolutions": {
		"@types/vfile-message": "1.0.1",
		"@types/react-native": "npm:@types/react@16.9.53",
		"@types/react": "16.9.53",
		"@types/react-dom": "16.9.8",
		"react": "16.14.0",
		"react-dom": "16.14.0",
		"react-is": "16.13.1"
	},
	"workspaces": [
		"packages/*/*"
	],
	"dependencies": {
		"@yarnpkg/pnpify": "^2.3.3"
	},
	"packageManager": "yarn@3.1.0"
}
