{
	"name": "@graspologic/monorepo",
	"version": "0.6.0",
	"private": true,
	"description": "Efficient WebGL-based Graph Data visualization + React Componentry",
	"repository": "https://github.com/microsoft/Graspologic-js",
	"author": "Chris Trevino (chtrevin@microsoft.com)",
	"contributors": [
		"David Tittsworth (datittsw@microsoft.com)",
		"Nathan Evans (naevans@microsoft.com)"
	],
	"license": "MIT",
	"scripts": {
		"preinstall": "npx only-allow yarn",
		"clean:": "yarn workspaces foreach -piv run clean",
		"build:": "yarn workspaces foreach -pivt run build",
		"bundle:": "yarn workspaces foreach -pv run bundle",
		"start:": "yarn workspaces foreach -piv run start",
		"test:": "yarn workspaces foreach -piv run test",
		"release:": "yarn workspaces foreach run npm publish --tolerate-republish --access public",
		"lint:": "essex lint",
		"lint:fix": "essex lint --fix",
		"unit:test": "jest --ci --coverage",
		"prettify": "essex prettify",
		"start_glsl": "yarn start:glsl",
		"start_storybook": "yarn start:storybook",
		"build_glsl": "yarn build:glsl",
		"git_is_clean": "essex git-is-clean",
		"ci": "npm-run-all -s build_glsl -p build: bundle: lint: test: unit:test -s git_is_clean",
		"update_sdks:": "yarn dlx @yarnpkg/sdks vscode"
	},
	"engines": {
		"node": ">= 12"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@essex/eslint-config": "^18.0.2",
		"@essex/eslint-plugin": "^18.0.2",
		"@essex/jest-config": "^18.0.2",
		"@essex/prettier-config": "^18.0.2",
		"@essex/scripts": "^18.2.1",
		"@types/enzyme": "^3.10.10",
		"@types/eslint": "^8.2.0",
		"@types/invariant": "^2.2.35",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@types/prettier": "^2.4.2",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"applicationinsights": "^1.8.10",
		"colors": "^1.4.0",
		"dts-bundle": "^0.7.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"eslint": "^8.3.0",
		"eslint-import-resolver-node": "^0.3.6",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jest": "^25.3.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^4.3.8",
		"jest": "^27.4.3",
		"jest-webgl-canvas-mock": "^0.2.3",
		"lint-staged": "^12.1.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"regenerator-runtime": "^0.13.9",
		"rollup": "^2.60.2",
		"shx": "^0.3.3",
		"typedoc-plugin-markdown": "^3.11.7",
		"typescript": "4.5.2"
	},
	"resolutions": {
		"@types/vfile-message": "1.0.1",
		"@types/react": "17.0.37",
		"@types/react-dom": "17.0.11",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-is": "17.0.2"
	},
	"workspaces": [
		"packages/*"
	],
	"packageManager": "yarn@3.1.1"
}
