{
	"name": "@graspologic/monorepo",
	"version": "0.7.0",
	"private": true,
	"description": "Efficient WebGL-based Graph Data visualization + React Componentry",
	"repository": "https://github.com/microsoft/Graspologic-js",
	"author": "Chris Trevino (chtrevin@microsoft.com)",
	"contributors": [
		"David Tittsworth (datittsw@microsoft.com)",
		"Nathan Evans (naevans@microsoft.com)"
	],
	"license": "MIT",
	"scripts": {
		"_turbo_ci": "turbo run ci",
		"rome_ci": "rome ci .",
		"rome_fix": "rome check . --apply-unsafe",
		"format": "rome format . --write",
		"preinstall": "npx only-allow yarn",
		"clean:": "turbo run clean",
		"build:": "turbo run build",
		"bundle:": "turbo run bundle",
		"start:": "turbo run start",
		"test:": "turbo run test",
		"release:": "yarn workspaces foreach -piv npm publish --tolerate-republish",
		"check:": "turbo run check",
		"fix:": "turbo run fix",
		"unit:test": "jest --ci --coverage",
		"start_glsl": "yarn start:glsl",
		"start_storybook": "yarn start:storybook",
		"build_glsl": "yarn build:glsl",
		"git_is_clean": "essex git-is-clean",
		"ci": "npm-run-all build_glsl _turbo_ci rome_ci git_is_clean",
		"update_sdks": "yarn dlx @yarnpkg/sdks vscode"
	},
	"engines": {
		"node": ">= 12"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@essex/eslint-config": "^21.0.1",
		"@essex/eslint-plugin": "^21.0.1",
		"@essex/jest-config": "^22.0.0",
		"@essex/scripts": "^26.0.0",
		"@types/enzyme": "^3.10.10",
		"@types/eslint": "^8.2.1",
		"@types/invariant": "^2.2.35",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.12",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
		"@typescript-eslint/parser": "^5.6.0",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
		"applicationinsights": "^1.8.10",
		"colors": "^1.4.0",
		"dts-bundle": "^0.7.3",
		"enzyme": "^3.11.0",
		"eslint": "^8.4.1",
		"eslint-import-resolver-node": "^0.3.6",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jest": "^25.3.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^4.3.8",
		"jest": "^27.4.3",
		"jest-webgl-canvas-mock": "^0.2.3",
		"lint-staged": "^12.1.2",
		"npm-run-all": "^4.1.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"regenerator-runtime": "^0.13.9",
		"rollup": "^2.60.2",
		"rome": "^12.1.3",
		"shx": "^0.3.3",
		"turbo": "^1.10.13",
		"typedoc-plugin-markdown": "^3.11.7",
		"typescript": "4.5.2"
	},
	"resolutions": {
		"@types/vfile-message": "1.0.1",
		"@types/react": "17.0.37",
		"@types/react-dom": "17.0.11",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-is": "17.0.2"
	},
	"workspaces": [
		"packages/*"
	],
	"packageManager": "yarn@4.0.0-rc.50"
}
