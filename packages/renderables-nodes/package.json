{
	"name": "@graspologic/renderables-nodes",
	"main": "src/index.ts",
	"description": "The node renderer contained within graspologic",
	"version": "0.7.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/graspologic-js.git",
		"directory": "packages/renderables/nodes"
	},
	"license": "MIT",
	"access": "public",
	"publishConfig": {
		"main": "dist/cjs/index.js",
		"types": "dist/types/index.d.ts",
		"exports": {
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"author": "Chris Trevino (chtrevin@microsoft.com)",
	"contributors": [
		"David Tittsworth (datittsw@microsoft.com)"
	],
	"scripts": {
		"clean": "essex clean lib dist",
		"build": "essex build --mode dual --skipExportCheck",
		"build_renderables": "yarn build",
		"start": "essex watch",
		"fix": "essex fix --strict",
		"check": "essex check --strict",
		"release": "yarn npm publish --tolerate-republish --access public"
	},
	"dependencies": {
		"@essex-js-toolkit/toolbox": "^3.0.1",
		"@essex/webpack-config": "^22.0.2",
		"@graspologic/animation": "workspace:^",
		"@graspologic/common": "workspace:^",
		"@graspologic/graph": "workspace:^",
		"@graspologic/luma-utils": "workspace:^",
		"@graspologic/memstore": "workspace:^",
		"@graspologic/renderables-base": "workspace:^",
		"@graspologic/renderer-glsl": "workspace:^",
		"@luma.gl/engine": "^8.5.21",
		"@luma.gl/gltools": "^8.5.21",
		"@luma.gl/shadertools": "^8.5.21",
		"@luma.gl/webgl": "^8.5.21"
	},
	"devDependencies": {
		"@essex/scripts": "^26.0.1",
		"@types/node": "^20.6.1",
		"glob": "^10.3.4",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.2.2",
		"webpack-bundle-analyzer": "^4.9.1"
	},
	"peerDependencies": {
		"@types/node": "*",
		"core-js": "^3.6.5",
		"regenerator-runtime": "^0.13.7"
	}
}
