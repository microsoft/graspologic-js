{
	"name": "@graspologic/layout-openord",
	"version": "0.7.0",
	"license": "MIT",
	"main": "src/index.ts",
	"access": "public",
	"publishConfig": {
		"main": "dist/cjs/index.js",
		"types": "dist/types/index.d.ts",
		"exports": {
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"author": "Chris Trevino (chtrevin@microsoft.com)",
	"contributors": [
		"David Tittsworth (datittsw@microsoft.com)"
	],
	"scripts": {
		"clean": "essex clean lib dist",
		"bundle_lib": "rollup -c rollup.config.js",
		"build_lib": "essex build --mode dual --skipExportCheck",
		"build": "npm-run-all -s build_lib -p bundle_lib",
		"start": "essex watch",
		"fix": "essex fix --strict",
		"check": "essex check --strict",
		"release": "yarn npm publish --tolerate-republish --access public"
	},
	"dependencies": {
		"@graspologic/common": "workspace:^",
		"@graspologic/graph": "workspace:^",
		"@graspologic/layout-core": "workspace:^",
		"core-js": "^3.32.2",
		"typedoc": "0.22.10"
	},
	"devDependencies": {
		"@essex/scripts": "^26.0.1",
		"@graspologic/testdata": "workspace:^",
		"@rollup/plugin-commonjs": "^15.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-sucrase": "^3.1.0",
		"@types/d3-scale-chromatic": "^1.5.1",
		"@types/node": "^20.6.1",
		"chalk": "^4.1.2",
		"d3-scale-chromatic": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.60.2",
		"rollup-plugin-terser": "^7.0.2"
	},
	"peerDependencies": {
		"@types/d3-scale-chromatic": "*",
		"@types/node": "*",
		"core-js": "^3.6.5",
		"regenerator-runtime": "^0.13.7"
	},
	"browserslist": [
		"last 2 Chrome versions"
	]
}
