		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-sucrase": "^3.1.0",
		"@types/node": "^20.6.0",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.60.2",
		"rollup-plugin-terser": "^7.0.2"
	},
	"peerDependencies": {
		"@types/node": "*",
		"core-js": "^3.6.5",
		"regenerator-runtime": "^0.13.7"
	},
	"browserslist": [
		"last 2 Chrome versions"
	]
