	"license": "MIT",
	"main": "src/index.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/graspologic-js.git",
		"directory": "packages/layout-fa2"
	},
	"access": "public",
	"publishConfig": {
		"main": "dist/cjs/index.js",
		"types": "dist/types/index.d.ts",
		"exports": {
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"author": "Chris Trevino (chtrevin@microsoft.com)",
	"contributors": [
		"David Tittsworth (datittsw@microsoft.com)"
	],
	"scripts": {
		"clean": "essex clean lib dist",
		"bundle_lib": "rollup -c rollup.config.js",
		"build_lib": "essex build --mode dual",
		"build": "npm-run-all -s build_lib -p bundle_lib",
		"start": "essex watch",
		"fix": "essex fix --strict",
		"check": "essex check --strict",
		"release": "yarn npm publish --tolerate-republish --access public"
	},
	"dependencies": {
		"@graspologic/common": "workspace:^",
		"@graspologic/graph": "workspace:^",
		"@graspologic/layout-core": "workspace:^",
		"typedoc": "0.22.10"
	},
	"devDependencies": {
		"@essex/scripts": "^26.0.0",
		"@rollup/plugin-commonjs": "^15.1.0",
